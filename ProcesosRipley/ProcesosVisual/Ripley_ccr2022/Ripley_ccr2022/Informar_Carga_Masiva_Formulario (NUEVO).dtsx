              SQLTask:SqlStatementSource="/****** Script para el comando SelectTopNRows de SSMS  ******/&#xA;truncate table mail_ingresa_postventa_masivo&#xA;insert into mail_ingresa_postventa_masivo&#xA;SELECT &#xA;  CONVERT(VARCHAR,GETDATE(),8)  AS HORA_PROCESO,&#xA;  CONVERT(VARCHAR,GETDATE(),5)  AS FECHA_PROCESO,&#xA;id_form&#xA;,[rut_cliente]&#xA;,[nombre_cli]&#xA;,[correo]&#xA;,form.oc&#xA;,bita.[numero_ticket]&#xA;,'MAIL - SE INGRESA POST VENTA MASIVO' AS [tipificacion_auris]  &#xA;from [dbo].[bitacora_carga_masiva] as bita&#xA;inner join [dbo].[DESCARGA_FORMULARIO] as form on bita.id_form=form.id&#xA;&#xA;  where CAST([FECHA_PROCEOS]+' '+[HORA_PROCESO] as datetime) = (select MAX(CAST([FECHA_PROCEOS]+' '+[HORA_PROCESO] as datetime)) from [dbo].[bitacora_carga_masiva])&#xA;&#xA;&#xA;-----------DECLARAR VARIABLES------------------&#xA;DECLARE @email NVARCHAR(MAX)&#xA;DECLARE @body NVARCHAR(MAX)&#xA;DECLARE @cliente NVARCHAR(MAX)&#xA;DECLARE @nombre NVARCHAR(MAX)&#xA;DECLARE @oc NVARCHAR(MAX)&#xA;DECLARE @tk_auris NVARCHAR(MAX)&#xA;declare @asunto nvarchar(mAX)&#xA;&#xA;--------------------------------------------------------------------------------------------------------------------------------------------------&#xA;--------------------------------------------------------------------------------------------------------------------------------------------------&#xA;------------------------------------------------------EN GESTION POSTVENTA MASIVO------------------------------------------------------------------&#xA;--------------------------------------------------------------------------------------------------------------------------------------------------&#xA;--------------------------------------------------------------------------------------------------------------------------------------------------&#xA;&#xA;&#xA;Declare cliente cursor fast_forward&#xA;       for &#xA;     &#xA;SELECT &#xA;&#xA;id_form&#xA;  &#xA;from mail_ingresa_postventa_masivo&#xA;&#xA;       open cliente&#xA;&#x9; &#xA;&#x9; &#xA;&#x9; Fetch next from cliente into @cliente&#xA;&#xA;       while (@@FETCH_STATUS &lt;&gt; -1)&#xA;       begin&#xA;&#xA;&#xA;----- Datos de Clientes ------------------------&#xA;&#xA;set @email = (select top 1 [correo] from mail_ingresa_postventa_masivo where id_form=@cliente);&#xA;set @nombre = (select top 1 [nombre_cli] from mail_ingresa_postventa_masivo where id_form=@cliente);&#xA;set @tk_auris = (select top 1 [numero_ticket] from mail_ingresa_postventa_masivo where id_form=@cliente);&#xA;set @oc = (select top 1 oc from mail_ingresa_postventa_masivo where id_form=@cliente);&#xA;set @asunto = char(39) + '[Ripley] Se ha ingresado tu requerimiento' + char(39);&#xA;&#xA;&#xA;----- Proceso de Email Estructura y Envio ------------&#xA;&#xA;SET @body =&#xA;'&#xA;&#xA;&#xA;&#xA;&lt;!DOCTYPE html&gt;&#xA;&lt;html lang=&quot;es&quot;&gt;&#xA;&lt;head&gt;&#xA;&lt;title&gt;Ripley.com&lt;/title&gt;&#xA;&lt;meta charset=&quot;utf-8&quot;&gt;&#xA;&lt;meta name=&quot;viewport&quot; content=&quot;width=device-width&quot;&gt;&#xA;&lt;link rel=&quot;icon&quot; href=&quot;https://home.ripley.cl/assets-mails-2020/img/favicon.ico&quot; type=&quot;image/x-icon&quot;&gt;&#xA;&lt;/head&gt;&#xA;&lt;style&gt;&#xA;  a[x-apple-data-detectors] {&#xA;  color: inherit !important;&#xA;  text-decoration: none !important;&#xA;  font-size: inherit !important;&#xA;  font-family: inherit !important;&#xA;  font-weight: inherit !important;&#xA;  line-height: inherit !important;&#xA;}&#xA;a:-webkit-any-link {&#xA;color: inherit !important;&#xA;text-decoration: none;&#xA;}&#xA;&lt;/style&gt;&#xA;&#xA;&lt;body style=&quot;margin: 0; padding: 0;&quot;&gt;&#xA;&#xA;&#xA; &lt;!-- ASUNTO MAIL RIPLEY --&gt;&#xA; &lt;div style=&quot;display: none; font-size: 1px; color:#333333; line-height: 1px; font-family: Arial, sans-serif; max-height: 0px; max-width: 0px; opacity: 0; overflow: hidden; mso-hide: all;&quot;&gt;&#xA;  [Solicitud recibida] Tu ticket electrónico
                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
campana.[cam_numcam] as campana,
recursos.rec_numrec as usuario,
cast(base.[Rut] as nvarchar) + ISNULL(cast(DV as nvarchar),'K') as Rut ,
base.[Nombre],
base.[Fono],
base.[CUD],
base.[SKU],
base.[OC],
base.[Email],
base.[lugar_compra],
left(base.Glosa,350) as glosa,
base.estado_caso,
motivo.[mot_nummot1],
motivo.[mot_nummot2],
motivo.[mot_nummot3],
base.numero_boleta,
base.gestion


FROM [cierre_creacion_auris].[dbo].[base_virtual_excel] as base
left join [dbo].[campana] as campana on base.campana=campana.cam_nomcam collate SQL_Latin1_General_CP1_CI_AS
inner join [dbo].[motivo] as motivo on campana.cam_numcam=motivo.mot_numcam 
inner join [dbo].[recurso] as recursos on base.usuario=recursos.[rec_userna] collate SQL_Latin1_General_CP1_CI_AS

where base.[Motivo 1]=motivo.[mot_desmot1] collate SQL_Latin1_General_CP1_CI_AS
	and	base.[Motivo 2]=motivo.[mot_desmot2] collate SQL_Latin1_General_CP1_CI_AS
	and base.[Motivo 3]=motivo.[mot_desmot3] collate SQL_Latin1_General_CP1_CI_AS</property>

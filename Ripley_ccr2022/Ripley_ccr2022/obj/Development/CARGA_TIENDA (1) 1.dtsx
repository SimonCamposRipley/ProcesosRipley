                          name="OpenRowsetVariable"></property>
                        <property
                          dataType="System.String"
                          description="Comando SQL para ejecutar."
                          name="SqlCommand"
                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select distinct * from (
SELECT distinct
id,
[RUT] as CUSTOMER_ID,
[CLIENTE] AS NOMBRE_CLIENTE,
coalesce([TELEFONO_CLIENTE],'Sin Info') as Telefono,
[Campana],
[id_mot1],
[id_mot2],
[id_mot3],
mot.num_responsable,
[EMAIL_CLIENTE] as email,
left(OC,len(oc)-2) as OC,
letra_oc,
coalesce(convert(varchar,[fecha_compra],103),'Sin Info') as fecha_compra,
tienda,
left(('CARGA FORMULARIO TIENDA // ID : '+CAST(id AS varchar)+' // ID REFUD: '+ coalesce([id_refund],'Sin Info') +' // FECHA RECEPCION:'+coalesce(convert(varchar,[fecha_compra],103),'Sin Info')+' // ANULACION REALIZADA:'+coalesce([nota_credito],'Sin Info')+' //COMENTARIO CLIENTE:'+coalesce(comentario_cliente,'Sin Info')),300) AS GLOSA
FROM [PROCESO_CARGA_MASIVA].[dbo].[DESCARGA_TIENDA] as form
left join [dbo].[DESCARGA_SGO] as mkp on LEFT(oc,LEN(oc)-2)=MKP.nroventa
left join [dbo].[motivos_formulario] as mot on form.tipificacion_auris=mot.CODIGO_TIPIFICACION
WHERE Estado_auris = 'crear' and rut is not null
union all
SELECT distinct
id,
[RUT] as CUSTOMER_ID,
[CLIENTE] AS NOMBRE_CLIENTE,
coalesce([TELEFONO_CLIENTE],'Sin Info') as Telefono,
[Campana],
[id_mot1],
[id_mot2],
[id_mot3],
mot.num_responsable,
[EMAIL_CLIENTE] as email,
LEFT(oc,LEN(oc)-2) as OC,
letra_oc,
coalesce(convert(varchar,[fecha_compra],103),'Sin Info') as fecha_compra,
tienda,
left(('CARGA FORMULARIO TIENDA // ID : '+CAST(id AS varchar)+' // ID REFUD: '+ coalesce([id_refund],'Sin Info') +' // FECHA RECEPCION:'+coalesce(convert(varchar,[fecha_compra],103),'Sin Info')+' // ANULACION REALIZADA:'+coalesce([nota_credito],'Sin Info')+' //COMENTARIO CLIENTE:'+coalesce(comentario_cliente,'Sin Info')),300) AS GLOSA
FROM [PROCESO_CARGA_MASIVA].[dbo].[DESCARGA_TIENDA] as form
left join [dbo].[DESCARGA_SGO] as mkp on left(LEFT(oc,LEN(oc)-2),9)=MKP.nroventa
left join [dbo].[motivos_formulario] as mot on form.tipificacion_auris=mot.CODIGO_TIPIFICACION
WHERE Estado_auris = 'crear' and rut is not null
) as a</property>
